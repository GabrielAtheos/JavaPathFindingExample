FRAMEWORK=/home/gabriel/Documents/CSUN/cs282/projects/Sturtevant282P2

all:	
	rm -rf *.class 
	javac -cp ${FRAMEWORK}:${CLASSPATH} Player.java
	javac -cp ${FRAMEWORK}:${CLASSPATH} WayPoint.java
	javac -cp ${FRAMEWORK}:${CLASSPATH} Map.java
	javac -cp ${FRAMEWORK}:${CLASSPATH} Sturtevant282P1.java
